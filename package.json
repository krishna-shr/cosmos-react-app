{
    "name": "react_app",
    "version": "1.0.0",
    "scripts": {
        "start-local:client": "webpack serve --config ./config/webpack/development/webpack.dev.client.js",
        "start-local:server": "nodemon src/server/app.ts",
        "start": "npm-run-all --parallel start-local:*",
        "build-dev:client": "webpack --config ./config/webpack/development/webpack.dev.client.js",
        "build-prod:client": "webpack --config ./config/webpack/production/webpack.prod.client.js",
        "build-dev:server": "webpack --config ./config/webpack/development/webpack.dev.server.js --watch --color",
        "build-prod:server": "webpack --config ./config/webpack/production/webpack.prod.server.js",
        "build-prod": "run-s build-prod:**",
        "serve:server": "node dist/server/* node --exec \"node --inspect dist/server/main.js\"",
        "test": "jest --config ./config/jest/jest.config.js",
        "test-watch": "jest --config ./config/jest/jest.config.js --watch",
        "test-report": "jest --config ./config/jest/jest.config.js --coverage",
        "prepare": "husky install",
        "lint:staged": "lint-staged",
        "lint": "eslint src/**/*.tsx --fix && src/**/*.ts --fix",
        "prettier": "prettier --write"
    },
    "dependencies": {
        "axios": "0.19.2",
        "axios-auth-refresh": "3.2.2",
        "connect-redis": "4.0.4",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5",
        "express": "4.17.1",
        "express-session": "1.17.1",
        "helmet": "3.22.0",
        "history": "5.3.0",
        "jwt-decode": "3.1.2",
        "log4js": "3.0.6",
        "mysql": "2.18.1",
        "qs": "6.10.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-redux": "7.2.6",
        "react-router-dom": "6.3.0",
        "redis": "3.0.2",
        "redux": "4.1.2",
        "redux-devtools-extension": "2.13.9",
        "redux-thunk": "2.4.1",
        "rotating-file-stream": "1.4.6",
        "styled-components": "^5.3.6"
    },
    "devDependencies": {
        "@babel/core": "7.17.5",
        "@babel/plugin-transform-runtime": "7.17.0",
        "@babel/preset-env": "7.16.11",
        "@babel/preset-react": "7.16.7",
        "@svgr/webpack": "6.2.1",
        "@testing-library/jest-dom": "5.16.2",
        "@testing-library/react": "11.2.6",
        "@testing-library/react-hooks": "7.0.1",
        "@testing-library/user-event": "14.2.1",
        "@types/cors": "2.8.6",
        "@types/express": "4.17.6",
        "@types/express-session": "1.17.4",
        "@types/jest": "27.4.1",
        "@types/node": "14.18.12",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.13",
        "@types/react-helmet": "6.1.5",
        "@types/react-redux": "7.1.23",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "babel-loader": "8.2.3",
        "brotli-webpack-plugin": "1.1.0",
        "clean-webpack-plugin": "4.0.0",
        "compression-webpack-plugin": "10.0.0",
        "css-loader": "6.6.0",
        "dotenv": "10.0.0",
        "eslint": "8.10.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.29.3",
        "eslint-plugin-react-hooks": "4.3.0",
        "express-static-gzip": "2.1.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "7.0.4",
        "jest": "27.5.1",
        "lint-staged": "11.2.6",
        "morgan": "1.10.0",
        "nodemon": "1.19.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.5.1",
        "react-test-renderer": "17.0.2",
        "style-loader": "3.3.1",
        "ts-jest": "27.1.3",
        "ts-loader": "9.2.7",
        "ts-node": "9.1.1",
        "typescript": "4.6.2",
        "webpack": "5.72.0",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.8.1",
        "webpack-manifest-plugin": "5.0.0",
        "webpack-merge": "5.8.0",
        "webpack-node-externals": "1.7.2"
    },
    "lint-staged": {
        "src/**/*.{js,ts,jsx,tsx}": [
            "npm run prettier"
        ],
        "*.*": [
            "git add"
        ]
    }
}
